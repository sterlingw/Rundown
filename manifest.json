{
    "manifest_version": 2,
    "name": "Rundown CSS",
    "description": "Show all element positioning CSS by hitting the rundown button.",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "icon.html",
        "default_title": "Rundown CSS"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ]
}
